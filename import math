import math

pi = 3.14
x = 1
y = 2
z = 3

print(round(pi))
print(math.ceil(pi))  # round the number up
print(math.floor(pi))  # round the number down
print(abs(pi))  # gives you the absolute value of the number
print(pow(pi,2))  # takes the power of the number
print(math.sqrt(pi))  # takes the square of the number
print(max(x,y,z))  # find the largest one
print(min(x,y,z))  # find the smallest one
